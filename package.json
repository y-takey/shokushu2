{
  "name": "shokushu2",
  "productName": "Shokushu2",
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rm -rf ./dist && mkdir ./dist",
    "dev:renderer": "yarn clean:dist && cp ./app/development.html ./dist/index.html && NODE_ENV=development webpack-dev-server --hot",
    "build": "yarn clean:dist && cp ./app/production.html ./dist/index.html && NODE_ENV=production webpack",
    "package": "yarn build && electron-builder build --publish never",
    "dev": "concurrently -n \"RENDERER,MAIN\" \"yarn dev:renderer\" \"yarn dev:main\"",
    "dev:main": "wait-on http://localhost:8080 && NODE_ENV=development electron main.js"
  },
  "main": "main.js",
  "author": {
    "name": "y-takey",
    "email": "no-reply@example.org",
    "url": "https://github.com/y-takey/shokushu2"
  },
  "description": "",
  "build": {
    "productName": "Shokushu2",
    "appId": "org.tky.Shokushu2",
    "directories": {
      "output": "release"
    },
    "files": [
      "main.js",
      "preload.js",
      "dist",
      "resources",
      "package.json"
    ],
    "mac": {
      "icon": "resources/icon.icns",
      "target": [
        "dmg"
      ]
    }
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@electron/typescript-definitions": "^8.9.6",
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "antd": "4.18.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.5.1",
    "core-js": "^3.20.1",
    "css-loader": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "fs-extra": "^10.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-hotkeys": "^2.0.0",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "nedb": "^1.8.0"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
