{
  "name": "shokushu2",
  "productName": "Shokushu2",
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rm -rf ./dist && mkdir ./dist",
    "dev:renderer": "yarn clean:dist && cp ./app/development.html ./dist/index.html && NODE_ENV=development webpack-dev-server --hot",
    "build": "yarn clean:dist && cp ./app/production.html ./dist/index.html && NODE_ENV=production webpack",
    "package": "yarn build && electron-builder build --publish never",
    "dev": "concurrently -n \"RENDERER,MAIN\" \"yarn dev:renderer\" \"yarn dev:main\"",
    "dev:main": "wait-on http://localhost:8080 && NODE_ENV=development electron main.js"
  },
  "main": "main.js",
  "author": {
    "name": "y-takey",
    "email": "no-reply@example.org",
    "url": "https://github.com/y-takey/shokushu2"
  },
  "description": "",
  "build": {
    "productName": "Shokushu2",
    "appId": "org.tky.Shokushu2",
    "directories": {
      "output": "release"
    },
    "files": [
      "main.js",
      "preload.js",
      "dist",
      "resources",
      "package.json"
    ],
    "mac": {
      "icon": "resources/icon.icns",
      "target": [
        "dmg"
      ]
    }
  },
  "devDependencies": {
    "@ant-design/icons": "^4.2.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@electron/typescript-definitions": "^8.7.4",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "antd": "4.3.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "electron": "^11.5.0",
    "electron-builder": "^22.7.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "fs-extra": "^9.0.1",
    "less": "^3.11.3",
    "less-loader": "^6.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.0.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-hotkeys": "^2.0.0",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.5",
    "wait-on": "^5.0.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "nedb": "^1.8.0"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
