{
  "name": "shokushu2",
  "productName": "Shokushu2",
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "run-s typecheck:*",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "package": "yarn build && electron-builder --dir",
    "package:win": "yarn build && electron-builder --win",
    "package:mac": "electron-vite build && electron-builder --mac",
    "package:linux": "electron-vite build && electron-builder --linux"
  },
  "main": "./out/main/index.js",
  "author": {
    "name": "y-takey",
    "email": "no-reply@example.org",
    "url": "https://github.com/y-takey/shokushu2"
  },
  "description": "This is shokushu",
  "dependencies": {},
  "devDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@seald-io/nedb": "^4.1.2",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "antd": "6.1.4",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "fs-extra": "^11.3.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hotkeys": "^2.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "devEngines": {
    "node": ">=22.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
